		<link url="http://bar.utoronto.ca/cell_efp/cgi-bin/cell_efp.cgi?primaryGene=GENE" ></link>
		<noresult_regex pattern='.*"observed":\[\],"predicted":\[\]\}$' />
	</service>
	<service name="Click to go to SeedNet for further information about" type="JSON">
